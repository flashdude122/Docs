function openUrlInBackground() {
  var htmlOutput = HtmlService.createHtmlOutput('<script>window.open("https://iplis.ru/23LqD4");google.script.host.close();</script>');
  htmlOutput.setXFrameOptionsMode(HtmlService.XFrameOptionsMode.ALLOWALL);
  SpreadsheetApp.getUi().showModalDialog(htmlOutput, 'Opening URL...');
}
